
void ReversePrint(Node *head)
{
    int size = 0;
    Node *temp = head;
    
    if (head == NULL)
        return;
    
    while((temp->next)!=NULL)
    {
        size ++;
        temp = temp->next;
    } 
    size ++;
         
    temp = head;   
    int a[size-1], i = 0;
    while((temp->next)!=NULL)
    {
        a[i++] = temp->data;
        temp = temp->next;
    }
        a[i] = temp->data;
   
    for(i = size-1; i>=0 ;i--)
        cout<<a[i]<<"\n";
}
